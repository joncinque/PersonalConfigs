	name = Jon Cinque
	email = jon.cinque@gmail.com
[credential]
	helper = cache --timeout=3600
[difftool]
	prompt = false
[diff]
    tool = vimdiff
[alias]
    st = status
    pl = pull --rebase
    logdog = log --decorate --pretty=oneline --abbrev-commit --graph
    dt = difftool
[push]
	default = simple
